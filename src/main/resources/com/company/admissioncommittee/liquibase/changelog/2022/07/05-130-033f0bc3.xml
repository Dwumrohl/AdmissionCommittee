<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="AdmissionCommittee" dbms="postgresql">
        <createIndex indexName="IDX_UC_BRANCH_CODE" tableName="BRANCH" unique="true">
            <column name="CODE"/>
        </createIndex>

        <modifySql>
            <append value="where DELETED_DATE is null"/>
        </modifySql>
    </changeSet>
    <changeSet id="2" author="AdmissionCommittee" dbms="postgresql">
        <createIndex indexName="IDX_UC_BRANCH_NAME" tableName="BRANCH" unique="true">
            <column name="NAME"/>
        </createIndex>

        <modifySql>
            <append value="where DELETED_DATE is null"/>
        </modifySql>
    </changeSet>
</databaseChangeLog>